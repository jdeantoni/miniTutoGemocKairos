            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="connector" sourceMapping="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='KairosSystem']/@defaultLayer/@nodeMappings[name='Actor']/@borderedNodeMappings[name='outputport']" targetMapping="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='KairosSystem']/@defaultLayer/@nodeMappings[name='Actor']/@borderedNodeMappings[name='inputport']" targetFinderExpression="[self.inputport/]" sourceFinderExpression="[self.outputport/]" domainClass="kairosTuto::Connector" useDomainElement="true">
