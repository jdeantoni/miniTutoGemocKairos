          <decorationDescriptions xsi:type="description_1:MappingBasedDecoration" name="Disabled breakpoint" position="WEST" preconditionExpression="service:self.hasDisabledBreakpoint" imageExpression="/org.eclipse.gemoc.executionframework.extensions.sirius/icons/breakpoint_disabled.gif" mappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='KairosSystem']/@defaultLayer/@nodeMappings[name='Actor'] //@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='KairosSystem']/@defaultLayer/@nodeMappings[name='Actor']/@borderedNodeMappings[name='inputport'] //@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='KairosSystem']/@defaultLayer/@nodeMappings[name='Actor']/@borderedNodeMappings[name='outputport'] //@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='KairosSystem']/@defaultLayer/@edgeMappings[name='connector']"/>
        </decorationDescriptionsSet>
        <customization>
